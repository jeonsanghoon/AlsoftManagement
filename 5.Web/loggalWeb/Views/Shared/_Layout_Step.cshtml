
<!DOCTYPE html>
<html>
<head>
    <!-- Standard Meta -->
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">

    <!-- Site Properties -->
    <title>@ViewBag.title</title>
    @{Html.RenderPartial("/Views/Shared/Partial/Header.cshtml"); }
    

</head>
<body>
    @RenderBody()
    @{Html.RenderPartial("/Views/Shared/Partial/Script.cshtml"); }
