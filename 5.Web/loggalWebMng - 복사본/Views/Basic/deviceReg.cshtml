            if($("#LOCAL_NAME3").val() != "")
            {
                localName = $("#LOCAL_NAME3").val() ;
            }

            if (localName != "")
            {
                DaumApi.Map.addr2coord((localName == "" ? "서울시" : localName), function (result) {
