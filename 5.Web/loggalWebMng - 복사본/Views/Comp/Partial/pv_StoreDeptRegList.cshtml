@using ALT.VO.Common;
@{
    List<T_STORE_DEPT> list = ViewBag.list;



    foreach (T_STORE_DEPT data in list)
    {
        <tr>

            <td><input type="text" class="number" value="@data.DEPT_SEARCH" placeholder="상위코드 + 자신의코드(2자리)로 입력" /></td>
            <td><input type="text" value="@data.DEPT_NAME" /></td>
            <td>@data.PARENT_DEPT_NAME</td>
            <td class="center">@data.LEVEL</td>
            <td class="center"><input type="checkbox" @Html.Raw(data.HIDE == true ? "checked" : "" ) /></td>
            <td class="center">@data.UPDATE_NAME</td>
            <td class="center">@data.UPDATE_DATE</td>
            <td class="center hide">@data.DEPT_CODE</td>
            <td class="center hide">@data.STORE_CODE</td>
        </tr>
    }



}
