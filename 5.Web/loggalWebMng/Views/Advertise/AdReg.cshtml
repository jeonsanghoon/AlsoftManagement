        var timezonoffset = 9;
        function adShareSave(savelist) {

            if ($("#AD_CODE").val() != "" && Number($("#AD_CODE").val()) <= 0) {
                $.MessageBox("배너정보를 먼저 저장하여 주시기 바랍니다.");
