        function AdSigninfoSaveCheck() {
            $(".signInfo *").removeClass("error");
            var rtn = true;
            if ($("#SIGN_IS_VERTICAL").val() == "") {
                $.MessageBox("디스플레이(가로/세로)를 선택하여 주시기 바랍니다.").done(function () { $("#SIGN_IS_VERTICAL").closest(".ui.dropdown").addClass("error"); })
