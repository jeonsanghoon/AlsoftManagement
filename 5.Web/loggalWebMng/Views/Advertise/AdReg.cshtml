@model EditTempleateModel
@using ALT.VO.loggal;
@using ALT.Framework.Data;
@{


    T_AD data;
    if (ViewBag.data == null)
    {
        data = new T_AD()
        {
            COMPANY_CODE = SessionHelper.LoginInfo.COMPANY_CODE
            ,
            STORE_CODE = SessionHelper.LoginInfo.STORE_CODE
