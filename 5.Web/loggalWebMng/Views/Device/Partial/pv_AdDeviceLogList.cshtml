
@{
	List<T_AD_DEVICE_LOG> list = (ViewBag.list == null) ? new List<T_AD_DEVICE_LOG>() : ViewBag.list;
	string totCnt = (list.Count() == 0 ? "0" : list.First().TOTAL_ROWCOUNT.ToString());
	<input type="hidden" id="total" value="@totCnt" />
	foreach (T_AD_DEVICE_LOG data in list)
	{
<tr>
	<td class="center">@data.UPDATE_DATE</td>
	<td class="center">@data.AD_CODE</td>
	<td class="left"><a href="/advertise/adreg2/@(data.AD_CODE)">@data.TITLE</a></td>
	<td class="center">@data.FR_DATE</td>
	<td class="center">@data.TO_DATE</td>
	<td class="center">@data.AD_FRAME_TYPE Frame</td>
	<td class="left"><a href="/basic/devicereg3/@(data.DEVICE_CODE)">@data.DEVICE_NAME</a></td>
	<td class="center">@data.STATUS</td>
	<td class="center">@data.AD_STORE_NAME</td>
	<td class="center">@data.DEVICE_STORE_NAME</td>
	<td class="center">@data.CLICK_CNT</td>
	<td class="center">@data.REMARK</td>
	<td class="center">@data.REMARK2</td>
</tr>
	}
}



