<body style="background-color:black">

    @{ Response.Buffer = true;}
    @RenderBody()
    @{Html.RenderPartial("/Views/Shared/Partial/Script.cshtml"); }
    @RenderSection("scripts", required: false)
    @{Html.RenderPartial("/Views/Shared/Partial/Analytics.cshtml"); }

    <script type="text/javascript">
        $("document").ready(function () {

            $("body").mouseover(function () {
                try {
                    window.external.notify(JSON.stringify({ method: "UsingWebView" }));
                } catch (e) { }
            });
            $("body").mouseout(function () {
                try {
                    window.external.notify(JSON.stringify({ method: "UsingWebView" }));
                } catch (e) { }

            });
        })
    </script>

    @{Html.RenderPartial("/Views/Shared/Partial/loader.cshtml"); }
</body>

